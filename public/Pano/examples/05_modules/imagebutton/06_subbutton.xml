<?xml version="1.0" encoding="utf-8"?>
<!-- 
	http://panozona.com/wiki/Module:ImageButton
-->
<SaladoPlayer>
	<global debug="true">
		<trace open="true"/>
	</global>
	<panoramas>
		<panorama id="checkerYellow" path="~panoramas/dz_checker_yellow/checker_yellow_f.xml"/>
	</panoramas>
	<modules>
		<ImageButton path="~modules/ImageButton-1.3.swf">
			<button id="logo1" path="~media/images/misc/panozona_logo.png">
				<subButtons>
					<subButton id="map1" path="~media/images/misc/button_map1.png" action="printHelloMap" mouse="onOver:printHelloMap"/>
					<subButton id="close" path="~media/images/misc/close.png" action="printHelloClose" move="horizontal:85" singleState="true"/>
				</subButtons>
			</button>
		</ImageButton>
	</modules>
	<actions>
		<action id="printHelloMap" content="SaladoPlayer.print(Hello I am subButton map1)"/>
		<action id="printHelloClose" content="SaladoPlayer.print(Hello I am subButton close)"/>
	</actions>
</SaladoPlayer>