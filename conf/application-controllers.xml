<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xmlns:context="http://www.springframework.org/schema/context"
     xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
         http://www.springframework.org/schema/context
         http://www.springframework.org/schema/context/spring-context-3.0.xsd">
         
 	<bean id="filter" class="controllers.Filter"/>
 	<bean id="interceptor" class="controllers.Interceptor"/>
 	
 	<bean id="moduleUser" class="controllers.ModuleUser"/>   
 	<bean id="moduleOrder" class="controllers.ModuleOrder"/>   
 	<bean id="moduleAds" class="controllers.ModuleAds"/>
 	<bean id="moduleCombo" class="controllers.ModuleCombo"/>
 	<bean id="moduleSns" class="controllers.ModuleSns"/>
 	
</beans>