<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.SbillitUserMapper">
    <select id="findAllUsers" parameterType="int" resultType="SbillitUser">
        select * from `sbillit_user` where 1=1
    </select>
    
    <select id="findUserById" parameterType="map" resultType="SbillitUser">
        select * from `sbillit_user` where id=#{id}
    </select>
</mapper>